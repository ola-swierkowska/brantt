{
  "$schema": "https://schemas.wp.org/wp/6.7/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "color": "#FFFFFF",
          "name": "Base",
          "slug": "base"
        },
        {
          "color": "#1B3454",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#22B4AB",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#ff4043",
          "name": "Heading red",
          "slug": "heading-red"
        },
        {
          "color": "#503AA8",
          "name": "Accent 3",
          "slug": "accent-3"
        },
        {
          "color": "#686868",
          "name": "Accent 4",
          "slug": "accent-4"
        },
        {
          "color": "#FBFAF3",
          "name": "Accent 5",
          "slug": "accent-5"
        },
        {
          "color": "color-mix(in srgb, currentColor 20%, transparent)",
          "name": "Accent 6",
          "slug": "accent-6"
        }
      ]
    },
    "layout": {
      "contentSize": "1200px",
      "wideSize": "1340px"
    },
    "spacing": {
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "name": "Tiny",
          "size": "10px",
          "slug": "20"
        },
        {
          "name": "X-Small",
          "size": "20px",
          "slug": "30"
        },
        {
          "name": "Small",
          "size": "30px",
          "slug": "40"
        },
        {
          "name": "Regular",
          "size": "clamp(30px, 5vw, 50px)",
          "slug": "50"
        },
        {
          "name": "Large",
          "size": "clamp(30px, 7vw, 70px)",
          "slug": "60"
        },
        {
          "name": "X-Large",
          "size": "clamp(50px, 7vw, 90px)",
          "slug": "70"
        },
        {
          "name": "XX-Large",
          "size": "clamp(70px, 10vw, 140px)",
          "slug": "80"
        }
      ],
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "writingMode": true,
      "defaultFontSizes": false,
      "fluid": true,
      "fontSizes": [
        {
          "fluid": false,
          "name": "Small",
          "size": "0.875rem",
          "slug": "small"
        },
        {
          "fluid": {
            "max": "1.125rem",
            "min": "1rem"
          },
          "name": "Medium",
          "size": "1rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "max": "1.375rem",
            "min": "1.125rem"
          },
          "name": "Large",
          "size": "1.38rem",
          "slug": "large"
        },
        {
          "fluid": {
            "max": "2rem",
            "min": "1.75rem"
          },
          "name": "Extra Large",
          "size": "1.75rem",
          "slug": "x-large"
        },
        {
          "fluid": {
            "max": "3rem",
            "min": "2.15rem"
          },
          "name": "Extra Extra Large",
          "size": "2.15rem",
          "slug": "xx-large"
        }
      ],
      "fontFamilies": [
        {
          "name": "Manrope",
          "slug": "manrope",
          "fontFamily": "Manrope, sans-serif",
          "fontFace": [
            {
              "src": [
                "file:./assets/fonts/manrope/Manrope-VariableFont_wght.woff2"
              ],
              "fontWeight": "200 800",
              "fontStyle": "normal",
              "fontFamily": "Manrope"
            }
          ]
        },
        {
          "name": "Fira Code",
          "slug": "fira-code",
          "fontFamily": "\"Fira Code\", monospace",
          "fontFace": [
            {
              "src": [
                "file:./assets/fonts/fira-code/FiraCode-VariableFont_wght.woff2"
              ],
              "fontWeight": "300 700",
              "fontStyle": "normal",
              "fontFamily": "\"Fira Code\""
            }
          ]
        }
      ]
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "color": {
      "background": "var:preset|color|base",
      "text": "var:preset|color|contrast"
    },
    "spacing": {
      "blockGap": "1.2rem",
      "padding": {
        "left": "var:preset|spacing|50",
        "right": "var:preset|spacing|50"
      }
    },
    "typography": {
      "fontFamily": "var:preset|font-family|manrope",
      "fontSize": "var:preset|font-size|large",
      "fontWeight": "300",
      "letterSpacing": "-0.1px",
      "lineHeight": "1.4"
    }
  }
}
